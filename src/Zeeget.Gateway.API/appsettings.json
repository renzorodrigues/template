{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "HttpClientSettings": {
    "KeycloakToken": {
      "BaseAddress": "http://localhost:8080",
      "RequestUri": "/realms/myrealm/protocol/openid-connect/token"
    }
  },
  "Authentication": {
    "Authority": "http://localhost:8080/realms/myrealm",
    "Audience": "account",
    "RequireHttpsMetadata": false
  }
}
